            <property name="visible">True</property>
            <property name="can_focus">False</property>
            <property name="tooltip_text" translatable="yes">Systém počítání znaků nefunguje na český jazyk přesně jako znaky, české znaky se totiž do zpráv kódují, jako znaky dva.</property>
            <property name="label" translatable="yes">Napsáno 0/125</property>
          </object>
          <packing>
            <property name="expand">True</property>
            <property name="fill">True</property>
            <property name="position">5</property>
          </packing>
        </child>
      </object>
    </child>
  </object>
</interface>
